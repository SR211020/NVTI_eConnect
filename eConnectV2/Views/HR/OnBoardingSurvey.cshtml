@section ScriptsBlock {
    <script src="~/js/formplugins/bootstrap-datepicker/bootstrap-datepicker.js"></script>
    <script>
        $(document).ready(function () {
            $('#btnSubmitOnboard').click(function () {
                GetOnboardSurveyData();
            });
            $('#btnSubmitOnboard1').click(function () {
                GetOnboardSurveyData1();
            });
            $(function () {
                runDatePicker();
            });
            var controls = {
                leftArrow: '<i class="fas fa-angle-left" style="font-size: 1.25rem"></i>',
                rightArrow: '<i class="fas fa-angle-right" style="font-size: 1.25rem"></i>'
            }
            var runDatePicker = function () {
                $('#OBdoj').datepicker({
                    todayHighlight: true,
                    orientation: "bottom left",
                    templates: controls,
                    autoclose: true
                });
            }
        });
        function btn_start() {
            var doj = $('#OBdoj').val();
            if (doj == '' || doj == undefined) {
                Swal.fire("Oops...", "Date of joining is mandatory!", "error");
            } else {
                var requestUrl = 'HR/IsUserValid';
                $.ajax({
                    url: requestUrl,
                    type: "POST",
                    data: { doj: doj },
                    success: function (data) {
                        if (data == "1") {
                            window.location.reload();
                        } else {
                            Swal.fire("Denied!", "Incorrect Date of joining", "error").then(function () {

                            })
                        }
                    },
                    error: function () {

                    }
                });
            }
        }
        function GetOnboardSurveyData() {
            var queList = [];
            var rowDetails = [];
            var isValid = true;
            for (var i = 1; i < 43; i++) {
                var selectedAns = $('#r' + i + '2').val();
                if (selectedAns == '0' || selectedAns == '') {
                    isValid = false;
                    break;
                }
            }
            if (isValid) {
                for (var i = 1; i < 43; i++) {
                    var que = $('#r' + i + '1').text();
                    var ans = $('#r' + i + '2').val();
                    var dimensionCategory = $('#r' + i + '3').text();
                    var experiencePillar = $('#r' + i + '4').text();
                    rowDetails = [que, ans, dimensionCategory, experiencePillar, '1', '0'];
                    queList.push(rowDetails);
                }
                var requestUrl = 'HR/SaveOnboardAns';
                $.ajax({
                    url: requestUrl,
                    type: "POST",
                    data: { queList: queList },
                    success: function (data) {
                        if (data == "1") {
                            window.location.reload();
                        } else {
                            Swal.fire("Saved!", "Survey submitted successfully", "success").then(function () {
                                window.location.replace("https://eConnect.nvtpower.com/");
                            })
                        }
                    },
                    error: function () {

                    }
                });
            }
